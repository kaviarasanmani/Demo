{
    "version": 2,
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "manage.py" }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "demo.settings"
    },
    "build": {
      "env": {
        "PIP_INSTALL": "psycopg2-binary"
      }
    }
  }
  